{
  "name": "@lazy-bee/basic",
  "version": "0.1.8",
  "main": "./src/main.js",
  "scripts": {
    "serve:src": "vue-cli-service serve --open",
    "build-src": "vue-cli-service build --target lib --name basic src/main.js",
    "lint:src": "vue-cli-service lint",
    "serve:demo": "start-storybook -c demo/config -p 9001",
    "build:demo": "build-storybook -c demo/config -o dist/demo",
    "install:package": "HUSKY_SKIP_INSTALL=true yarn install",
    "release:component": "npm run build:src && npm run build:demo"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "element-ui": "^2.3.6",
    "js-base64": "^2.4.5",
    "sortablejs": "^1.7.0",
    "vue": "^2.5.13",
    "vue-bus": "^1.1.0",
    "vue-echarts": "^3.0.7",
    "vuetify": "1.2.3"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.3.15",
    "@storybook/addon-options": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/vue": "^3.3.15",
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-prettier": "^3.0.0-beta.6",
    "babel-core": "^7.0.0-0",
    "babel-plugin-component": "^1.1.0",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-vue": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-vue": "^4.4.0",
    "lint-staged": "^7.0.0",
    "storybook-readme": "^3.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.13"
  },
  "license": "MIT"
}
